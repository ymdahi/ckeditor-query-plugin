{
  "name": "ckeditor-query-plugin",
  "version": "0.0.0",
  "description": "Simle plugin to add query snippets on Libretexts",
  "scripts": {
    "lint": "npm run lint:styles; npm run lint:scripts",
    "lint:styles": "stylelint src",
    "lint:scripts": "eslint src",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js --colors",
    "start": "webpack-dev-server --open --config webpack/webpack.config.dev.js",
    "deploy:test": "bin/deploy.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wbkd/webpack-starter.git"
  },
  "keywords": [
    "webpack",
    "startkit",
    "frontend",
    "es6",
    "javascript",
    "webdev"
  ],
  "author": "ckeditor-query-plugin Development Team",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/LibreTexts/ckeditor-query-plugin/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.11.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^5.2.4",
    "eslint": "^7.9.0",
    "eslint-loader": "^4.0.2",
    "file-loader": "^6.1.0",
    "html-loader": "^1.3.0",
    "html-webpack-plugin": "^4.4.1",
    "postcss-loader": "^4.0.2",
    "sass-loader": "^10.0.2",
    "style-loader": "^2.0.0",
    "stylelint": "^13.7.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-webpack-plugin": "^2.1.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.11.5",
    "ckeditor4": "^4.17.0",
    "core-js": "^3.6.5",
    "glob-parent": "5.1.2",
    "hosted-git-info": "3.0.8",
    "sass": "^1.32.13"
  }
}
